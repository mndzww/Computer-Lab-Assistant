sks_alpro = 3
sks_fismat = 3

#matkul_alpro
huruf_mutu_alpro = "A"
if huruf_mutu_alpro == "A":
    nilai_mutu_alpro = 4
elif (huruf_mutu_alpro == "B"):
    nilai_mutu_alpro = 3
elif (huruf_mutu_alpro == "C"):
    nilai_mutu_alpro = 2
elif (huruf_mutu_alpro == "D"):
    nilai_mutu_alpro =1
elif (huruf_mutu_alpro == "E"):
    nilai_mutu_alpro = 0
else:
    print("Nilai di luar jangkauan")
#print(nilai_mutu_alpro)

#matkul_fismat
huruf_mutu_fismat = "B"
if huruf_mutu_fismat == "A":
    nilai_mutu_fismat = 4
elif (huruf_mutu_fismat == "B"):
    nilai_mutu_fismat = 3
elif (huruf_mutu_fismat == "C"):
    nilai_mutu_fismat = 2
elif (huruf_mutu_fismat == "D"):
    nilai_mutu_fismat =1
elif (huruf_mutu_fismat == "E"):
    nilai_mutu_fismat = 0
else:
    print("Nilai di luar jangkauan")
#print(nilai_mutu_fismat)

bobot_nilai_alpro = nilai_mutu_alpro * sks_alpro
bobot_nilai_fismat = nilai_mutu_fismat * sks_fismat

total_bobot_nilai = bobot_nilai_fismat + bobot_nilai_alpro
total_sks = sks_alpro + sks_fismat

#IPK
IPK = total_bobot_nilai / total_sks
print(IPK)
